  "description": "TODO",
  "main": "index.ts",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*/index.js"
  },
  "scripts": {
    "clean": "rm -Rf dist && rm -Rf coverage && rm -Rf jest-output.txt",
    "clean:ps": "@powershell if (Test-Path ./dist) {Remove-Item ./dist -Recurse -Force}; if (Test-Path ./coverage) {Remove-Item ./coverage -Recurse -Force}; if (Test-Path ./docs) {Remove-Item ./docs -Recurse -Force}",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:file": "npm run test > jest-output.txt 2>&1",
    "build:dev": "webpack --mode=development",
    "pipeline:lint": "echo \"linting...\" && npm run _internal-install-sdk && npx eslint \"src/**\"",
    "_internal-install-sdk": "echo \"installing...\" && npm install --loglevel notice",
    "lint": "ts-standard -p ./tsconfig.eslint.json",
    "lint:fix": "ts-standard src --fix",
    "build:prod": "npm run clean && npm run test && rollup -c ",
    "build:prod:ps": "npm run clean:ps && npm run test && rollup -c ",
    "build:prod:dirty": "npm run test && rollup -c",
    "build:prod:testless": "npm run clean && rollup -c",
    "doc": "typedoc --out docs src",
    "doc:prod": "npm run clean && npm run doc",
    "doc:prod:ps": "npm run clean:ps && npm run doc"
  },
